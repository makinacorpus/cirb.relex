<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xml:lang="en" lang="en"
      i18n:domain="plone"
      metal:use-macro="here/main_template/macros/master">
  <head><title></title>
    <metal:block fill-slot="style_slot">
      <link href="++resource++cirb.relex/fancytree/skin-win8/ui.fancytree.min.css" rel="stylesheet" type="text/css" />
    </metal:block>
    <metal:block fill-slot="javascript_head_slot">
      <script src="++resource++cirb.relex/fancytree/lib/jquery-ui.custom.js"></script>
      <script src="++resource++cirb.relex/fancytree/jquery.fancytree.min.js"></script>
    </metal:block>
  </head>
  <body>
    <metal:content-core fill-slot="content-core">
      <div id="projects_tree">
        <ul>
          <li tal:repeat="project context/getFolderContents">
            <span><a href="" tal:attributes="href project/getURL" tal:content="project/Title">Project</a></span>
            <ul tal:define="context project" tal:condition="project/getFolderContents">
              <li tal:repeat="project context/getFolderContents">
                <span><a href="" tal:attributes="href project/getURL" tal:content="project/Title">Project</a></span>
              </li>
            </ul>
          </li>
        </ul>
      </div>

      <script>
        $(document).ready(function(){
          $('#projects_tree').fancytree();
        });
      </script>
    </metal:content-core>
  </body>
</html>
